{
  "dependencies": {
    "@openzeppelin/contracts": "^2.3.0",
    "bignumber": "^1.1.0",
    "browserify": "^16.5.0",
    "eslint-plugin-node": "^9.1.0",
    "express": "^4.17.1",
    "ganache-cli": "^6.6.0",
    "nodemon": "^1.19.1",
    "truffle": "^5.0.35",
    "truffle-assertions": "^0.9.1",
    "web3": "^1.2.1"
  },
  "scripts": {
    "build": "browserify src/*.js -o public/bundle.js",
    "dev": "yarn build && nodemon --watch src public build --exec \"yarn run build && yarn run start\"",
    "start": "node app.js",
    "setup": "cd contracts && truffle compile && truffle deploy && cd - && yarn build",
    "generate-docs": "doxygen Doxyfile"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "prettier": "^1.18.2",
    "prettier-plugin-solidity": "^1.0.0-alpha.32"
  }
}
